import React from "react";

export default function ChatSidebar({ onNewChat }) {
  return (
    <div className="sidebar">
      <div className="nav-icons">
        <button className="nav-btn" onClick={onNewChat} title="New Chat">
          <img src="/src/assets/icons/plus.svg" alt="New Chat" />
        </button>
      </div>
    </div>
  );
}